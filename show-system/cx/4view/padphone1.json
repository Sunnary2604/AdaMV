{
  "Modality": {
    "Interaction_modality": "desktop",
    "Size": {
      "width": 768,
      "height": 1024
    }
  },
  "Interface": [
    {
      "Encoding": {
        "url": "./demo5/bar-2.csv",
        "x": {
          "field": "Spending Amount (M)",
          "datum": "quantitative"
        },
        "y": {
          "field": "Category",
          "datum": "nominal"
        },
        "color": {
          "field": "Candidate",
          "datum": "nominal",
          "scale": {
            "range": ["#4e79a7","#b8163a"]
          }
        }
      },
      "ViewType": {
        "type": "side by side bar"
      },
      "BoundingBox": {
        "CenterPosition": {
          "x": 0.5,
          "y": 0.25
        },
        "Size": {
          "width": 1,
          "height": 0.5
        }
      }
    },
    {
      "transform": {
        "data": {
          "url": "./us-10m.json",
          "format": {
            "type": "topojson",
            "feature": "states"
          }
        }
      },
      "projection": {
        "type": "albersUsa"
      },
      "ViewType": {
        "type": "geoshape"
      },
      "Encoding": {
        "url": "./demo5/map.csv",
        "color": {
          "field":"Spending",
          "datum": "quantitative",
          "legend": {
            "format": "s"
          },
          "scale": {
            "range": ["#fec44f","#993404"]
          }
        }
      },
      "BoundingBox": {
        "CenterPosition": {
          "x": 0.5,
          "y": 0.75
        },
        "Size": {
          "width": 1,
          "height": 0.5
        }
      }
    }
  ]
}